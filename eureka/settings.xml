<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0
                              https://maven.apache.org/xsd/settings-1.0.0.xsd">
  <servers>
    <server>
      <id>deploymentRepo</id> <!-- This should match the id in your pom.xml's distributionManagement -->
      <username>your_nexus_username</username>
      <password>your_nexus_password</password>
    </server>
  </servers>
  <mirrors>
    <mirror>
      <id>nexus</id>
      <mirrorOf>*</mirrorOf>
      <url>http://192.168.1.160:1932/repository/maven-public/</url> <!-- Replace with your Nexus URL -->
    </mirror>
  </mirrors>
</settings>
